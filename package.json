{
  "name": "@qantasairways/runway",
  "version": "0.53.0-beta.3",
  "description": "Qantas Style Guide",
  "main": "./index.js",
  "module": "./esm/index.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:es && npm run build:umd && npm run build:prepare-bundle",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./lib --ignore \"**/*.spec.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./lib/esm --ignore \"**/*.spec.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./lib/es --ignore \"**/*.spec.js\"",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c ./rollup.config.js",
    "build:prepare-bundle": "cross-env copyfiles package.json CHANGELOG.md LICENSE README.md lib && ln -s node_modules lib/node_modules",
    "clean": "rimraf lib && mkdir lib",
    "watch": "watch 'node ./rollup' ./src/components",
    "semantic-release": "semantic-release",
    "test": "TZ=Australia/Sydney jest --env=jsdom",
    "test:update": "jest -u --env=jsdom",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "prebuild": "npm run clean",
    "start": "node ./server/index.js",
    "heroku-postbuild": "styleguidist build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qantasairways/runway.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qantasairways/runway/issues"
  },
  "homepage": "https://github.com/qantasairways/runway#readme",
  "dependencies": {
    "body-scroll-lock": "^2.6.1",
    "downshift": "^3.1.12",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "highlight-words-core": "^1.2.2",
    "lodash.debounce": "4.0.8",
    "nanoid": "^2.1.9",
    "prop-types": "^15.7.2",
    "rc-input-number": "^4.5.1",
    "react-highlight-words": "^0.16.0",
    "react-switch": "4.1.0",
    "react-transition-group": "^2.6.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-object-assign": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.4.4",
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-angular": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.27",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.27",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "copyfiles": "^2.1.1",
    "cross-env": "^6.0.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^3.0.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.27",
    "lint-staged": "^8.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-styleguidist": "^8.0.6",
    "rollup": "^1.28.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-visualizer": "^0.9.2",
    "semantic-release": "^15.13.31",
    "watch": "^1.0.2",
    "webpack": "^4.29.5",
    "webpack-blocks": "^2.0.0-rc"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.27",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "sideEffects": false,
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "jest-emotion"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add",
      "jest --env=jsdom --bail --findRelatedTests"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "lint"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
